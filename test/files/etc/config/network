	option target '::/0'

config interface 'stabridge'
	option proto 'relay'
	option network 'lan wan'

config interface 'mygre'
	option ipaddr '10.1.1.1'
	option peeraddr '10.2.2.1'
	option proto 'gre'

config interface 'mygre_static'
	option proto 'static'
	option device '@mygre'
