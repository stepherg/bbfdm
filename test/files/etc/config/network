config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'wan'
	option proto 'dhcp'
	option hostname 'iopsysWrt-00220775DC6C'
	option vendorid 'eg400'
	option device 'eth0'
	option reqopts '43'

config interface 'wan6'
	option proto 'dhcpv6'
	option device '@wan'

