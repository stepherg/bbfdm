#!/bin/sh

. /usr/share/libubox/jshn.sh

case "$1" in
        list)
                echo '{ "status" : {}, "stats": {}, "scanresults": {} }'
        ;;
        call)
                case "$2" in
                        status)
                                cat /tmp/wifi.radio.test1.status.data 2>/dev/null
                        ;;
                        stats)
                                cat /tmp/wifi.radio.test1.stats.data 2>/dev/null
                        ;;
                        scanresults)
                                cat /tmp/wifi.radio.test1.scanresults.data 2>/dev/null
                        ;;
                esac
        ;;
esac
