#!/bin/sh

. /usr/share/libubox/jshn.sh

case "$1" in
        list)
                echo '{ "steer_summary" : {} }'
        ;;
        call)
                case "$2" in
                        steer_summary)
                                cat /tmp/mapcontroller_steer_summary.data 2>/dev/null
                        ;;
                esac
        ;;
esac

